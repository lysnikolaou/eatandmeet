{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "default-case": "error",
        "max-classes-per-file": "warn",
        "no-multi-spaces": "error",
        "array-bracket-spacing": "error",
        "block-spacing": "error",
        "lines-between-class-members": "error"


    }
}
